{
  "name": "rssb-backend-challenge",
  "version": "1.0.0",
  "description": "Geo-Localizing Payments with Real-Time and Multiple System Notification Integration for a Large Scale System.",
  "main": "index.js",
  "repository": "git@github.com:placideirandora/rssb-backend-challenge.git",
  "author": "Placide IRANDORA <34879554+placiderapson@users.noreply.github.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "start": "node .",
    "dev": "nodemon",
    "sequelize": "node_modules/.bin/sequelize",
    "db:create": "yarn sequelize db:create payments",
    "db:drop": "yarn sequelize db:drop payments",
    "db:migrate": "yarn sequelize db:migrate",
    "db:migrate:undo": "yarn sequelize db:migrate:undo",
    "db:migrate:rollback": "yarn sequelize db:migrate:undo:all",
    "db:seed": "yarn sequelize db:seed:all",
    "db:seed:undo": "yarn sequelize db:seed:undo",
    "db:seed:undo:all": "yarn sequelize db:seed:undo:all",
    "db:reset": "yarn db:drop && yarn db:create && yarn db:migrate && yarn dev"
  },
  "dependencies": {
    "express": "^4.17.1",
    "express-validator": "^6.12.1",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.6.5",
    "sequelize-cli": "^6.2.0",
    "sequelize-typescript": "^2.1.0",
    "socket.io": "^4.1.3",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.4.13",
    "@types/sequelize": "^4.28.10",
    "@types/socket.io": "^3.0.2",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.12",
    "ts-node": "^10.1.0",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5"
  }
}
